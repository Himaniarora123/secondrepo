pipeline {
    agent any
    stages {
        stage('stack-creation') {
            steps {
                sh "aws cloudformation create-stack --stack-name myfirststack  --template-body file://demo1.yaml --region 'us-east-1' --parameters ParameterKey= S3BucketName, ParameterValue= testbucket80765 "
            }
        }
    }
}
